<%= link_to 'New', new_withdraw_path %>
<h1>Index</h1>

<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody>
    <% @withdraws.each do |w| %>
      <tr>
        <td><%= w.date %></td>
        <td><%= w.amount %></td>
        <td><%= link_to 'Edit', edit_withdraw_path(w.id) %></td>
        <td><%= link_to 'Delete', w, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
